{
  "compilerOptions": {
    "target": "ES2017",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": false,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "plugins": [
      { "name": "next" }
    ],
    "paths": {
      "@/*": ["./*"]
    }
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    ".next/types/**/*.ts",
    "app/(real-estate)/create-wellness-lounge/page.jsx",
    "app/(real-estate)/categories-list/page.jsx",
    "app/(real-estate)/alumni-registration/page.jsx",
    "app/(real-estate)/student-registration/page.jsx",
    "components/ui/student-registration-form.jsx",
    "components/ui/registration-form.jsx",
    "app/(real-estate)/cancel-order/page.jsx",
    "app/(real-estate)/coupon-list/page.jsx",
    "app/(real-estate)/create-coupon/page.jsx",
    "app/(real-estate)/create-order/page.jsx",
    "app/(real-estate)/create-user/page.jsx",
    "app/(real-estate)/login/page.jsx",
    "app/(real-estate)/payment/page.jsx",
    "app/(real-estate)/payment-gateway-list/page.jsx",
    "app/(real-estate)/profile/page.jsx",
    "app/(real-estate)/registration/page.jsx",
    "app/(real-estate)/test/page.jsx",
    "app/(real-estate)/calendar/page.jsx",
    "app/(real-estate)/student-order/page.jsx",
    "app/(real-estate)/update-lounge/page.jsx",
    "app/(real-estate)/update-coupon/page.jsx",
    "app/(real-estate)/update-order/page.jsx",
    "app/(real-estate)/wellness-lounge-list/page.jsx",
    "app/(real-estate)/view-wellness-lounge/page.jsx",
    "app/(real-estate)/view-user/page.jsx",
    "app/(real-estate)/view-order/page.jsx",
    "app/(real-estate)/user-list/page.jsx",
    "app/(real-estate)/update-user/page.jsx",
    "app/layout.jsx",
    "components/common-components/checkbox.jsx",
    "components/common-components/timePicker.jsx",
    "components/common-components/toast.jsx",
    "components/common-components/useDebounce.jsx",
    "components/common-sidebar/side-menu.jsx",
    "components/ui/alumni-registration-form.jsx",
    "components/ui/command.jsx",
    "components/common-components/auth-provider.jsx",
    "components/common-components/dropdown.jsx",
    "components/common-components/header.jsx",
    "components/common-components/textArea.jsx",
    "components/common-components/root-layout.jsx",
    "components/common-components/multi-select.jsx",
    "components/common-components/primaryButton.jsx",
    "components/ui/custom-full-calendar.jsx",
    "app/(real-estate)/order-list/page.jsx",
    "app/page.jsx",
    "components/common-sidebar/team-switcher.jsx",
    "components/common-sidebar/nav-user.jsx",
    "components/common-sidebar/nav-projects.jsx",
    "components/common-sidebar/nav-main.jsx"
  ],
  "exclude": ["node_modules"]
}
